
target "ProjectFile" do
    platform :ios, '8.0'

pod 'AFNetworking' #网络请求
pod 'MBProgressHUD' #提示信息
pod 'MJRefresh' #刷新
pod 'MJExtension' #字典数组一键转模型
pod 'SDWebImage'    #图片处理
pod 'ImagePlayerView' #轮播图效果
pod 'CorePlot' #绘图使用
pod 'MWPhotoBrowser' #支持本地和远程图片展示，使用起来也比较方便
# http://www.cnblogs.com/lwme/p/ios-third-party-libraries.html 第三方集合
pod 'UIActivityIndicator-for-SDWebImage' #为SDWebImage显示加载效果
pod 'MMDrawerController' #侧滑栏
pod 'TMQuiltView' #瀑布流
pod 'IQKeyboardManager' #自动处理键盘事件
pod 'JKCategories'

end



